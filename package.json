{
  "name": "eslint-plugin-zod-v4",
  "version": "0.3.0",
  "description": "ESLint plugin for Zod v4 best practices and migration from v3",
  "author": "Matheus Pimenta <studio.kodaai@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "zod",
    "zod-v4",
    "validation",
    "schema",
    "typescript",
    "migration"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Just-mpm/eslint-plugin-zod-v4"
  },
  "bugs": {
    "url": "https://github.com/Just-mpm/eslint-plugin-zod-v4/issues"
  },
  "homepage": "https://github.com/Just-mpm/eslint-plugin-zod-v4#readme",
  "engines": {
    "node": ">=18.18.0"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@typescript-eslint/rule-tester": "^8.32.1",
    "@typescript-eslint/utils": "^8.32.1",
    "eslint": "^9.27.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  }
}
